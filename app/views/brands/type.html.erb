<div class="type-wrapper">
  <div class="type-central">
    <span class="type-label">Article No.</span>
    <%= render 'icons/search_watermark' %>
    <%= form_with url: "/brands/#{@brand.id}/type_search", method: :post do |form| %>
      <%= form.text_field :barcode, class: "type-textfield" %>
      <%= form.submit  class: 'type-submit' %>
    <% end %>
  </div>
</div>
